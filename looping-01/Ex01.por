programa {
  funcao inicio() {

    /*QUADRADO – Crie uma aplicação que percorra do número 15 ao número 50 e apresente o valor ao
    quadrado de cada um dos valores entre 15 e 50.*/

    inteiro i
    real quadrado

    para(i=15; i<=50; i++){
      quadrado = i*i
      escreva(quadrado," ")
    }

  }
}
