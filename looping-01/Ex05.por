programa {
  funcao inicio() {
    
    /*EXIBIR ÍMPARES – Crie uma aplicação que percorra os valores em uma faixa de 1 a 20 e apresente
    todos os valores ímpares existentes dentro desta faixa de valores*/

    inteiro i

    para(i=1; i<=20; i++){
      se(i%2!=0){
        escreva(i," ")
      }
    }

  }
}
